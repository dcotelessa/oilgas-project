# Oil & Gas Customer Migration - Environment Template
# Copy this file to .env and update values for your environment

# Application Settings
APP_ENV=local
APP_PORT=8000
LOG_LEVEL=debug

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/database_name?sslmode=disable
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5433/test_database?sslmode=disable

# Database Components
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database
DB_USER=your_user
DB_PASSWORD=your_password
DB_SSLMODE=disable

# Multi-tenant
DEFAULT_TENANT_ID=your-tenant

# Security
JWT_SECRET=change-this-in-production

# API
API_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000

# Development
ENABLE_DEBUG_ROUTES=true

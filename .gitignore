.env
.env.production
.env.prod
.env.staging
.env.dev
.env.local

# Real database files
*.mdb
*.accdb
*.db
*.sqlite
*.sqlite3

# Database backups and dumps
backup_*.sql
dump_*.sql
*_backup.sql
*_production.sql
real_data/
production_data/
backup/
backups/

# Real customer data exports
customer_export_*.csv
real_customers.csv
production_*.csv
sensitive_*.csv

# Configuration files with real endpoints/keys
config.production.json
config.prod.json
secrets.json
credentials.json

# SSL certificates and keys
*.pem
*.key
*.crt
*.p12
*.pfx
ssl/
certs/
certificates/

